{
	"$schema": "https://json.schemastore.org/prettierrc",
	"useTabs": true,
	"semi": false,
	"tabWidth": 4,
	"singleQuote": true,
	"printWidth": 200,
	"trailingComma": "none",
	"vueIndentScriptAndStyle": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"overrides": [
		{
			"files": "*.yml",
			"options": {
				"useTabs": false,
				"tabWidth": 2
			}
		}
	]
}
