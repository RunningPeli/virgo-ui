{
	"name": "@virgo-ui/nuxt",
	"version": "0.1.2",
	"description": "Nuxt 3 Module for Virgo",
	"license": "MIT",
	"private": false,
	"homepage": "https://virgoui.dev",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RunningPeli/virgo-ui.git",
		"directory": "packages/virgo-nuxt"
	},
	"bugs": {
		"url": "https://github.com/RunningPeli/virgo-ui/issues"
	},
	"keywords": [
		"nuxt 3",
		"virgo-nuxt",
		"vue 3"
	],
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/module.mjs",
			"require": "./dist/module.cjs"
		}
	},
	"main": "./dist/module.cjs",
	"module": "./dist/module.mjs",
	"types": "./dist/types.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm dev:prepare && nuxt-module-build",
		"dev": "pnpm dev:prepare && nuxi dev playground",
		"dev:build": "nuxi build playground",
		"dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
	},
	"dependencies": {
		"@virgo-ui/theme-base": "workspace:*",
		"@floating-ui/dom": "1.2.8",
		"@iconify-json/bx": "latest",
		"@nuxt/kit": "3.15.4",
		"@virgo-ui/vue": "workspace:*"
	},
	"devDependencies": {
		"@nuxt/devtools": "2.1.1",
		"@nuxt/devtools-ui-kit": "2.1.1",
		"@nuxt/module-builder": "0.8.4",
		"@nuxt/schema": "3.15.4",
		"@unocss/nuxt": "0.51.13",
		"nuxt": "3.15.4"
	}
}
