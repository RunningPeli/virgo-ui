{
	"name": "@virgo-ui/theme-base",
	"version": "0.1.2",
	"description": "Official theme config for Virgo",
	"type": "module",
	"private": false,
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./dist/style.css": "./dist/style.css"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"*.d.ts",
		"*.css"
	],
	"scripts": {
		"build": "unbuild",
		"stub": "unbuild --stub"
	},
	"author": "Virgo Labs <https://github.com/RunningPeli>",
	"license": "MIT",
	"funding": "https://github.com/sponsors/RunningPeli",
	"homepage": "https://virgoui.dev",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RunningPeli/virgo-ui.git",
		"directory": "packages/theme-base"
	},
	"bugs": {
		"url": "https://github.com/RunningPeli/virgo-ui/issues"
	},
	"keywords": [
		"component library",
		"ui components",
		"vuejs",
		"vue"
	],
	"devDependencies": {
		"@types/fs-extra": "11.0.1",
		"@unocss/core": "0.51.13",
		"@unocss/reset": "0.51.13",
		"fs-extra": "11.1.1",
		"rollup-plugin-scss": "4.0.0",
		"sass": "1.62.1"
	}
